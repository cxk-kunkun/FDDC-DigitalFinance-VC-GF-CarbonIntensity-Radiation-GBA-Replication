      name:  <unnamed>
       log:  C:\Users\Administrator\Desktop\a.txt
  log type:  text

. 
. * 加载数据
. use "`DATA'", clear

. xtset 代码 年份

Panel variable: 代码 (strongly balanced)
 Time variable: 年份, 2011 to 2022
         Delta: 1 unit

. 
. *====================================================================
. * 1. 变量预处理 (Variable Pre-processing)
. * 功能：生成对数、中心化变量、计算残差等
. *====================================================================
. 
. * 1.1 基础变量组内中心化 (Within-City Centering)
. * 用于构建交互项时减少多重共线性
. local basevars ln能耗强度 绿色全要素BOM ln数字金融 绿色金融 ln地区生产总值 ln公路运量源数据 ln风险投资额 ln绿色专利

. foreach v of local basevars {
  2.     capture confirm variable `v'
  3.     if _rc {
  4.         continue
  5.     }
  6.     capture confirm variable `v'_c
  7.     if _rc {
  8.         quietly su `v'
  9.         gen `v'_c = `v' - r(mean)
 10.     }
 11. }

. 
. * 1.2 数字金融核心变量中心化 (用于构建 DFz_Level 和 DFz_Contrast)
. capture drop lnDF_citymean lnDF_wc

. bysort 代码: egen lnDF_citymean = mean(ln数字金融)

. gen lnDF_wc = ln数字金融 - lnDF_citymean

. 
. * 1.3 生成物流效率残差 (Logistics Efficiency Residual)
. * 对应正文 Table 1 描述统计中的 LogisticsEff
. quietly xtreg ln公路运量源数据 ln地区生产总值_c i.年份, fe

. predict ln物流效率_c, residual

. quietly su ln物流效率_c

. replace ln物流效率_c = ln物流效率_c - r(mean)
(8 real changes made)

. 
. * 1.4 机制变量中心化
. local mech_vars_centered 产业结构高级化 第二产业占比

. foreach v of local mech_vars_centered {
  2.     capture confirm variable `v'
  3.     if _rc {
  4.         continue
  5.     }
  6.     capture confirm variable `v'_c
  7.     if _rc {
  8.         quietly su `v'
  9.         gen `v'_c = `v' - r(mean)
 10.     }
 11. }

. 
. *====================================================================
. * 2. 定义控制变量 (Define Controls)
. *====================================================================
. * 对应正文 Table 1 和 Table 2 的控制变量
. local controls_base "ln地区生产总值_c  第二产业占比_c ln物流效率_c 环境规则强度 产业结构高级化_c"

. 
. local __ctrl ""

. foreach v of local controls_base {
  2.     capture confirm variable `v'
  3.     if !_rc local __ctrl "`__ctrl' `v'"
  4.     else di as error "WARNING: 控制变量不存在 -> `v'"
  5. }

. 
. global controls "`__ctrl'"

. global fe_controls "i.年份"

. 
. di as txt ">>> controls      = $controls"
>>> controls      =  ln地区生产总值_c 第二产业占比_c ln物流效率_c 环境规则强度 产业结构高级化_c

. di as txt ">>> fe_controls   = $fe_controls"
>>> fe_controls   = i.年份

. 
. *====================================================================
. * 3. 定义程序 (Programs Definition)
. * 核心程序：mk_basis_z (生成权重和核心解释变量)
. * 辅助程序：mk_mech_z (生成机制交互项), run_xtreg (回归封装)
. *====================================================================
. 
. capture program drop mk_basis_z

. program define mk_basis_z
  1.     * 程序功能：根据权重类型(nested/geo/econ)和衰减参数(k)生成 DFz_Level 和 DFz_Contrast
.     syntax, k(real) [weight_type(string)]
  2.     if "`weight_type'" == "" local weight_type "nested"
  3.     
.     * 设置权重变量名
.     if "`weight_type'" == "nested" {
  4.         local wgz_var "广州嵌套权重"
  5.         local wsz_var "深圳嵌套权重"
  6.     }
  7.     else if "`weight_type'" == "geo" {
  8.         local wgz_var "广州地理距离权重"
  9.         local wsz_var "深圳地理距离权重"
 10.     }
 11.     else if "`weight_type'" == "econ" {
 12.         local wgz_var "广州经济距离权重"
 13.         local wsz_var "深圳经济距离权重"
 14.     }
 15.     
.     * 生成归一化权重
.     tempvar Wgz_pow min_gz max_gz range_gz
 16.     quietly {
 17.         bysort 年份: egen `min_gz' = min(`wgz_var')
 18.         bysort 年份: egen `max_gz' = max(`wgz_var')
 19.         gen double `range_gz' = `max_gz' - `min_gz'
 20.         replace `range_gz' = 1 if `range_gz' < 1e-9
 21.         gen double `Wgz_pow' = ((`wgz_var' - `min_gz') / `range_gz')^`k'
 22.     }
 23.     
.     tempvar Wsz_pow min_sz max_sz range_sz
 24.     quietly {
 25.         bysort 年份: egen `min_sz' = min(`wsz_var')
 26.         bysort 年份: egen `max_sz' = max(`wsz_var')
 27.         gen double `range_sz' = `max_sz' - `min_sz'
 28.         replace `range_sz' = 1 if `range_sz' < 1e-9
 29.         gen double `Wsz_pow' = ((`wsz_var' - `min_sz') / `range_sz')^`k'
 30.     }
 31. 
.     * 构造 Level (协同) 和 Contrast (方向) 变量
.     capture drop DFz_Level DFz_Contrast L1_DFz_Level L1_DFz_Contrast
 32.     capture drop DFz_GZ DFz_SZ
 33.     
.     tempvar Level_Var Contrast_Var
 34.     gen double `Level_Var' = `Wgz_pow' + `Wsz_pow'
 35.     gen double `Contrast_Var' = (`Wsz_pow' - `Wgz_pow') / (`Level_Var' + 1e-6)
 36.     
.     gen double DFz_Level = lnDF_wc * `Level_Var'
 37.     gen double DFz_Contrast = lnDF_wc * `Contrast_Var'
 38.     
.     * 单核心变量 (用于单核模型检验)
.     gen double DFz_GZ = lnDF_wc * `Wgz_pow'
 39.     gen double DFz_SZ = lnDF_wc * `Wsz_pow'
 40.     
.     * 生成滞后项 (用于 GTFP 回归)
.     sort 代码 年份
 41.     by 代码: gen double L1_DFz_Level = DFz_Level[_n-1]
 42.     by 代码: gen double L1_DFz_Contrast = DFz_Contrast[_n-1]
 43. end

. 
. capture program drop mk_mech_z

. program define mk_mech_z
  1.     * 程序功能：生成机制变量与 Level/Contrast 的交互项
.     capture drop Level_mech_* Contrast_mech_* L1_Level_mech_* L1_Contrast_mech_*
  2.     local mech_vars ln物流效率_c ln风险投资额_c ln绿色专利_c 绿色金融_c 产业结构高级化_c 第二产业占比_c
  3.     foreach mech in `mech_vars' {
  4.         capture confirm variable `mech'
  5.         if _rc continue
  6.         
.         local mech_clean = subinstr("`mech'", " ", "_", .)
  7.         local mech_clean = subinstr("`mech_clean'", "-", "_", .)
  8.         quietly {
  9.             gen double Level_mech_`mech_clean' = DFz_Level * `mech'
 10.             gen double Contrast_mech_`mech_clean' = DFz_Contrast * `mech'
 11.             sort 代码 年份
 12.             by 代码: gen double L1_Level_mech_`mech_clean' = Level_mech_`mech_clean'[_n-1]
 13.             by 代码: gen double L1_Contrast_mech_`mech_clean' = Contrast_mech_`mech_clean'[_n-1]
 14.         }
 15.     }
 16. end

. 
. capture program drop get_mech_varlist

. program define get_mech_varlist, rclass
  1.     * 获取生成的交互项变量列表
.     local mech_base ln物流效率_c ln风险投资额_c ln绿色专利_c 绿色金融_c 产业结构高级化_c 第二产业占比_c
  2.     local energy_list ""
  3.     local gtfp_list ""
  4.     foreach v of local mech_base {
  5.         local v_clean = subinstr("`v'", " ", "_", .)
  6.         local v_clean = subinstr("`v_clean'", "-", "_", .)
  7.         capture confirm variable Level_mech_`v_clean'
  8.         if !_rc {
  9.             local energy_list `energy_list' Level_mech_`v_clean' Contrast_mech_`v_clean'
 10.             local gtfp_list `gtfp_list' L1_Level_mech_`v_clean' L1_Contrast_mech_`v_clean'
 11.         }
 12.     }
 13.     return local energy "`energy_list'"
 14.     return local gtfp "`gtfp_list'"
 15. end

. 
. capture program drop run_xtreg

. program define run_xtreg
  1.     * 通用固定效应回归程序
.     syntax, depvar(string) indepvars(string) [model_name(string) lag(string) sample(string)]
  2.     local new_indepvars `indepvars'
  3.     if "`lag'" == "L1" {
  4.         local new_indepvars = subinstr("`new_indepvars'", "DFz_Level", "L1_DFz_Level", .)
  5.         local new_indepvars = subinstr("`new_indepvars'", "DFz_Contrast", "L1_DFz_Contrast", .)
  6.     }
  7.     
.     if "`sample'" != "" {
  8.         xtreg `depvar' `new_indepvars' $controls $fe_controls if `sample', fe vce(cluster 代码)
  9.     }
 10.     else {
 11.         xtreg `depvar' `new_indepvars' $controls $fe_controls, fe vce(cluster 代码)
 12.     }
 13.     if "`model_name'" != "" estimates store `model_name'
 14. end

. 
